{
    "name": "dapp",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "env_dev": "node ./scripts/set-environment.ts dev",
        "env_test": "node ./scripts/set-environment.ts test",
        "env_production": "node ./scripts/set-environment.ts production",
        "dev": "cross-env yarn env_dev && yarn next dev -p 3002",
        "test": "cross-env yarn env_test && yarn next dev -p 3002",
        "build-dev": "cross-env yarn env_dev && yarn next build",
        "build-test": "cross-env yarn env_test && yarn next build",
        "build-production": "cross-env yarn env_production && yarn next build",
        "start": "next start",
        "lint": "next lint",
        "prettier": "cross-env find . \\( -name node_modules -o -name contracts -o -name __generated__ -o -name .next \\) -prune -or \\( -name '*.js' -o -name '*.jsx' -o -name '*.tsx' -o -name '*.ts' -o -name '*.tsx' -o -name '*.ts' -o -name '*.html' -o -name '*.scss' -o -name '*.css' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write"
    },
    "dependencies": {
        "@hookform/resolvers": "^2.8.0",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "@material-ui/pickers": "^3.3.10",
        "@material-ui/styles": "^4.11.4",
        "@reduxjs/toolkit": "^1.6.1",
        "@solana/web3.js": "^1.24.1",
        "@styled-jsx/plugin-sass": "^4.0.1",
        "@types/clean-css": "^4.2.5",
        "@types/react-copy-to-clipboard": "^5.0.1",
        "@types/react-redux": "^7.1.18",
        "@types/react-router-dom": "^5.1.8",
        "axios": "^0.21.1",
        "clean-css": "^5.1.5",
        "clsx": "^1.1.1",
        "next": "11.1.2",
        "next-redux-wrapper": "^7.0.2",
        "react": "17.0.2",
        "react-copy-to-clipboard": "^5.0.4",
        "react-dom": "17.0.2",
        "react-redux": "^7.2.4",
        "react-router-dom": "^5.2.1",
        "redux-injectors": "^1.3.0",
        "redux-saga": "^1.1.3",
        "sass": "^1.38.2",
        "styled-jsx": "^4.0.1",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@types/react": "17.0.19",
        "@typescript-eslint/eslint-plugin": "^4.30.0",
        "cross-env": "^7.0.3",
        "eslint": "7.32.0",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-next": "11.1.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.24.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.25.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "prettier": "^2.3.2",
        "typescript": "4.4.2"
    }
}
