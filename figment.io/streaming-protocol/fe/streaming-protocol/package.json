{
    "name": "streaming-protocol",
    "private": true,
    "scripts": {
        "env_dev": "node ./scripts/set-environment.ts dev",
        "env_test": "node ./scripts/set-environment.ts test",
        "env_production": "node ./scripts/set-environment.ts production",
        "dev": "cross-env yarn env_dev && yarn next dev -p 3002",
        "test": "cross-env yarn env_test && yarn next dev -p 3002",
        "build-dev": "cross-env yarn env_dev && yarn next build",
        "build-test": "cross-env yarn env_test && yarn next build",
        "build-production": "cross-env yarn env_production && yarn next build",
        "start": "next start",
        "lint": "next lint",
        "prettier": "cross-env find . \\( -name node_modules -o -name contracts -o -name __generated__ -o -name .next \\) -prune -or \\( -name '*.js' -o -name '*.jsx' -o -name '*.tsx' -o -name '*.ts' -o -name '*.tsx' -o -name '*.ts' -o -name '*.html' -o -name '*.scss' -o -name '*.css' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write"
    },
    "dependencies": {
        "@emotion/cache": "^11.7.1",
        "@emotion/react": "^11.7.1",
        "@emotion/server": "^11.4.0",
        "@emotion/styled": "^11.6.0",
        "@hookform/resolvers": "^2.8.8",
        "@mui/icons-material": "^5.3.1",
        "@mui/material": "^5.3.1",
        "@mui/styles": "^5.3.0",
        "@reduxjs/toolkit": "^1.7.1",
        "@solana/wallet-adapter-base": "^0.9.2",
        "@solana/wallet-adapter-react": "^0.15.2",
        "@solana/wallet-adapter-react-ui": "^0.9.4",
        "@solana/wallet-adapter-wallets": "^0.14.2",
        "@solana/web3.js": "^1.32.0",
        "@styled-jsx/plugin-sass": "^4.0.2",
        "@types/clean-css": "^4.2.5",
        "@types/mz": "^2.7.4",
        "@types/react-redux": "^7.1.22",
        "axios": "^0.25.0",
        "borsh": "^0.7.0",
        "clean-css": "^5.2.4",
        "clsx": "^1.1.1",
        "date-fns": "^2.28.0",
        "mz": "^2.7.0",
        "next": "12.0.9",
        "next-redux-wrapper": "^7.0.5",
        "react": "17.0.2",
        "react-dark-mode-toggle-2": "^2.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "^7.25.3",
        "react-redux": "^7.2.6",
        "redux-injectors": "^1.3.0",
        "redux-saga": "^1.1.3",
        "sass": "^1.49.0",
        "styled-jsx": "^5.0.0",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/node": "17.0.13",
        "@types/react": "17.0.38",
        "cross-env": "^7.0.3",
        "eslint": "8.8.0",
        "eslint-config-next": "12.0.9",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "prettier": "^2.5.1",
        "typescript": "4.5.5"
    }
}
