{
    "name": "fe",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "env_dev": "node ./scripts/set-environment.ts dev",
        "env_test": "node ./scripts/set-environment.ts test",
        "env_production": "node ./scripts/set-environment.ts production",
        "dev": "cross-env yarn env_dev && yarn next dev -p 3002",
        "test": "cross-env yarn env_test && yarn next dev -p 3002",
        "build-dev": "cross-env yarn env_dev && yarn next build",
        "build-test": "cross-env yarn env_test && yarn next build",
        "build-production": "cross-env yarn env_production && yarn next build",
        "start": "next start",
        "lint": "next lint",
        "prettier": "cross-env find . \\( -name node_modules -o -name contracts -o -name __generated__ -o -name .next \\) -prune -or \\( -name '*.js' -o -name '*.jsx' -o -name '*.tsx' -o -name '*.ts' -o -name '*.tsx' -o -name '*.ts' -o -name '*.html' -o -name '*.scss' -o -name '*.css' -o -name '*.json' -o -name '*.md' \\) -print | xargs prettier --write"
    },
    "dependencies": {
        "@emotion/react": "^11.5.0",
        "@emotion/styled": "^11.3.0",
        "@hookform/resolvers": "^2.8.2",
        "@mui/icons-material": "^5.0.4",
        "@mui/material": "^5.0.4",
        "@reduxjs/toolkit": "^1.6.2",
        "@solana/web3.js": "^1.30.0",
        "@styled-jsx/plugin-sass": "^4.0.1",
        "@types/clean-css": "^4.2.5",
        "@types/mz": "^2.7.4",
        "@types/react-redux": "^7.1.20",
        "axios": "^0.23.0",
        "borsh": "^0.6.0",
        "clean-css": "^5.2.2",
        "clsx": "^1.1.1",
        "date-fns": "^2.25.0",
        "mz": "^2.7.0",
        "next": "11.1.2",
        "next-redux-wrapper": "^7.0.5",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "^7.17.5",
        "react-redux": "^7.2.5",
        "redux-injectors": "^1.3.0",
        "redux-saga": "^1.1.3",
        "sass": "^1.43.3",
        "yup": "^0.32.11"
    },
    "devDependencies": {
        "@types/react": "17.0.31",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "cross-env": "^7.0.3",
        "eslint": "8.1.0",
        "eslint-config-airbnb-typescript": "^14.0.1",
        "eslint-config-next": "11.1.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "eslint-plugin-react-hooks": "^4.2.0",
        "prettier": "^2.4.1",
        "typescript": "4.4.4"
    }
}
